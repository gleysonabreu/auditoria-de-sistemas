{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types",
      "./src/@types"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@classes/*": ["./src/classes/*"],
    }
  }
}
